{
  "name": "@webmuds/api-mock",
  "version": "0.1.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test c8 mocha",
    "test:ci": "NODE_ENV=test mocha",
    "lint": "standardx --fix",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webmuds/api-mock.git"
  },
  "author": "webmuds.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webmuds/api-mock/issues"
  },
  "homepage": "https://github.com/webmuds/api-mock#readme",
  "dependencies": {
    "nock": "13.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/eslint-parser": "7.14.7",
    "@types/chai": "4.2.21",
    "@types/mocha": "8.2.3",
    "@types/node": "16.3.1",
    "@webmuds/api-client": "github:webmuds/api-client#0.1.3",
    "c8": "7.7.3",
    "chai": "4.3.4",
    "mocha": "9.0.2",
    "release-it": "14.10.0",
    "standardx": "7.0.0"
  },
  "standardx": {
    "env": "mocha",
    "parser": "@babel/eslint-parser"
  },
  "babel": {},
  "eslintConfig": {
    "rules": {
      "no-var": 0,
      "prefer-const": 0,
      "no-unused-expressions": 0,
      "no-unused-vars": 0
    }
  },
  "mocha": {
    "recursive": true,
    "reporter": "dot",
    "colors": true
  },
  "c8": {
    "all": true,
    "include": [
      "index.js",
      "src"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagAnnotation": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "preRelease": true
    },
    "npm": {
      "publish": false
    }
  },
  "engines": {
    "node": ">= 14"
  }
}
