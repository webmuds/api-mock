{
  "name": "@webmuds/api-mock",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test c8 mocha",
    "test:ci": "NODE_ENV=test mocha",
    "lint": "standardx --fix",
    "release": "release-it --config ./node_modules/@webmuds/development/config/release-it/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webmuds/api-mock.git"
  },
  "author": "webmuds.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webmuds/api-mock/issues"
  },
  "homepage": "https://github.com/webmuds/api-mock#readme",
  "dependencies": {
    "nock": "13.1.3"
  },
  "devDependencies": {
    "@webmuds/api-client": "github:webmuds/api-client#0.1.3",
    "@webmuds/development": "github:webmuds/development#0.1.1",
    "release-it": "14.10.0"
  },
  "babel": {
    "extends": "./node_modules/@webmuds/development/config/babel"
  },
  "eslintConfig": {
    "extends": "./node_modules/@webmuds/development/config/eslint"
  },
  "mocha": {
    "recursive": true,
    "reporter": "dot",
    "colors": true
  },
  "c8": {
    "all": true,
    "include": [
      "index.js",
      "src"
    ]
  },
  "engines": {
    "node": ">= 14"
  }
}
